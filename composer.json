{
  "name": "autoprotect-group/knapsack",
  "license": "MIT",
  "type": "library",
  "description": "Collection library for PHP",
  "keywords": ["collections","sequences", "map", "reduce"],
  "homepage": "https://github.com/AutoProtect-Group/Knapsack",
  "authors": [
    {
      "name": "Dusan Kasan",
      "email": "dusan@kasan.sk",
      "homepage": "http://kasan.sk",
      "role": "Developer"
    }
  ],
  "require-dev": {
    "phpspec/phpspec": "^7.2",
    "squizlabs/php_codesniffer": "^3.6",
    "phpmd/phpmd" : "^2.12",
    "phpunit/phpunit": "^9.5",
    "symfony/console": "^6.0",
    "ext-dom": "*"
  },
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "files": [
      "src/collection_functions.php",
      "src/utility_functions.php"
    ],
    "psr-4": {
      "DusanKasan\\Knapsack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\DusanKasan\\Knapsack\\": "tests/",
      "spec\\DusanKasan\\Knapsack\\": "tests/spec/"
    }
  }
}
